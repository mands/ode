-- look in correct dirs for src
:set -idist/build/ode3/ode3-tmp:src:dist/build/autogen

-- warnings
:set -W
:set -fno-warn-unused-imports
:set -fno-warn-unused-binds
:set -fno-warn-unused-matches
:set -fno-warn-overlapping-patterns
:set -fno-warn-incomplete-patterns

-- useful settings
:set +t
-- :set +m
-- :set prompt "> "

-- custom cmds
:def source readFile

-- load ode and configure imports
:l src/Main
import Utils.CommonImports
