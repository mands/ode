module Test3 {
    /* Externally called component to generate channel current */
    component getCurrent(V,Y) {
        /* Setup initial values */
        init X1 = 0.3
        init X2 = 0.2
        init X3 = 0.6
        /* Setup state values (based on ODE/SDE/RRE form) */
        ssa {rate : exp(V)/V    +3} = X3 -> X2
        ssa {rate : 0.1} = X2 -> X3
        ssa {rate : 1.1} = X1 -> X2
        ssa {rate : 0.1} = X2 -> X1
        /* Calculate channel current */
        val current = 7.0*1.0*(X3)*(V-1.0)
        return current
        }
    }
/* EOF */