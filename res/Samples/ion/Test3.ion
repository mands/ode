// our sample ion channel
// cool, eh?!

channel test3 {
  sim_type : ssa,
  density : 1,
  equilibrium_potential : 1,
  channel_conductance : 7,
  subunits : 1,
  initial_states: { X1 : 0.3, X2 : 0.2, X3: 0.6 }
  additional_inputs: {Y}
  open_states : {X3},
  transitions : {
    {transition: X3 <-> X2, f_rate: exp(V)/V    +3, r_rate : 0.1},
    {transition: X1 <-> X2, f_rate: 1.1, r_rate : 0.1},
  },
}

